{
  "name": "oclockpong",
  "scripts": {
    "dev": "cross-env NODE_ENV=development webpack serve --config config/webpack.dev.js",
    "start": "cross-env NODE_ENV=production webpack serve --config config/webpack.prod.js",
    "build": "cross-env NODE_ENV=production webpack --config config/webpack.prod.js",
    "lint": "eslint '{src,__test__}/**/*.js'",
    "lint:fix": "eslint --fix '{src,__test__}/**/*.js'",
    "test": "cross-env NODE_ENV=test jest --testTimeout=10000 --detectOpenHandles",
    "test:watch": "cross-env NODE_ENV=test jest --testTimeout=10000 --watchAll --detectOpenHandles",
    "clean": "rm -rf dist",
    "clean:all": "rm -rf dist node_modules yarn.lock package-lock.json",
    "prepare": "husky install"
  },
  "devDependencies": {
    "@babel/core": "^7.16.0",
